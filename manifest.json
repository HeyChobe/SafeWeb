{
    "name": "SafeWeb",
    "version": "1.0",
    "description": "An extension to help prevent eye strain and follow the 20-20-20 rule.",
    "manifest_version": 2,
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "icons": {
      "16": "icons/favicon-16x16.png",
      "32": "icons/favicon-32x32.png",
      "36": "icons/favicon-36x36.png",
      "48": "icons/favicon-48x48.png",
      "72": "icons/favicon-72x72.png",
      "96": "icons/favicon-96x96.png",
      "144": "icons/favicon-144x144.png"
    },
    "browser_action": {
      "default_icon": {
        "16": "icons/favicon-16x16.png",
        "32": "icons/favicon-32x32.png",
        "36": "icons/favicon-36x36.png",
        "48": "icons/favicon-48x48.png",
        "72": "icons/favicon-72x72.png",
        "96": "icons/favicon-96x96.png",
        "144": "icons/favicon-144x144.png"
      },
      "default_popup": "popup.html",
      "default_title": "WebAid"
    },
    "permissions": [
      "tabs",
      "alarms",
      "storage"
    ]
  }